    "dotenv": "^16.3.1",
    "electron": "^28.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.14.2",
    "yamljs": "^0.3.0"
